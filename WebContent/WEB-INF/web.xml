<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>MondoMVC</display-name>


	<servlet>
		<servlet-name>Continente</servlet-name>
		<servlet-class>it.objectmethod.mondo.servlets.ContinenteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Continente</servlet-name>
		<url-pattern>/ContinenteServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Nazione</servlet-name>
		<servlet-class>it.objectmethod.mondo.servlets.NazioneServlet</servlet-class>
		 
	</servlet>

	<servlet-mapping>
		<servlet-name>Nazione</servlet-name>
		<url-pattern>/NazioneServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
	
			<servlet-name>Citta</servlet-name>
			<servlet-class>it.objectmethod.mondo.servlets.CittaServlet</servlet-class>
	
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Citta</servlet-name>
		<url-pattern>/CittaServlet</url-pattern>
    </servlet-mapping>

	
</web-app>
